{
  "manifest_version": 2,

  "name": "Meme Relief",
  "description": "Replaces meme with a much better picture :)",
  "version": "0.0.1",
  "web_accessible_resources": [
    "*.jpg",
    "*.JPG",
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ]
}
